{
    "name": "williambeeler/larablog",
    "description": "A Laravel Blog Package",
    "keywords": ["framework", "laravel", "blog"],
    "license": "MIT",
    "homepage": "https://williambeeler.com",
    "authors": [
        {
            "name": "William Beeler",
            "email": "will@williambeeler.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.6.15"
    },
    "autoload": {
        "psr-4": {
            "WilliamBeeler\\LaraBlog\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "WilliamBeeler\\LaraBlog\\LarablogServiceProvider"
            ],
            "aliases": {
              "LaraBlog": "WilliamBeeler\\LaraBlog\\Facade"
            }
        }
    },
    "require-dev": {
        "illuminate/framework": "5.5.x"
    }    
}
